{
    "name": "appjs",
    "version": "0.1.0",
    "description": "",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "build": "rm -rf dist; mkdir dist; tsc -p tsconfig.json; npm test",
        "test": "mocha -r ts-node/register 'tests/**/*.ts' --timeout 6000",
        "postinstall": "npm run build; node dist/index config.create",
        "start": "node dist/index start --config server",
        "exec": "node dist/index exec --config cli",
        "help": "node dist/index help",
        "clear": "node dist/index config.clear",
        "request": "node dist/index request",
        "config": "node dist/index config.get",
        "reset": "node dist/index exec reset",
        "update": "node dist/index exec update"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Aplenture/AppJS.git"
    },
    "author": "Aplenture",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Aplenture/AppJS/issues"
    },
    "homepage": "https://github.com/Aplenture/AppJS#readme",
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/chai-as-promised": "^7.1.5",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.18",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "mocha": "^10.2.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "backendjs": "github:Aplenture/BackendJS"
    }
}